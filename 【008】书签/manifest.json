{
    "manifest_version": 2,
    "name": "书签",
    "version": "1.0",
    "description": "书签",
    "icons": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "g书签牛逼",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": [
            "js/book.js"
        ]},
    "permissions":[
        "bookmarks"
    ]

}